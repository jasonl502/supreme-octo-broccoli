# Define the pride colors
pride_colors = [
    "\033[91m",  # Red
    "\033[93m",  # Yellow
    "\033[92m",  # Green
    "\033[94m",  # Blue
    "\033[95m",  # Purple
    "\033[96m",  # Cyan
]

# Print "Hello, World!" in pride colors
for i, char in enumerate("Hello, World!"):
    print(f"{pride_colors[i % len(pride_colors)]}{char}", end="")
print("\033[0m")  # Reset color to default
